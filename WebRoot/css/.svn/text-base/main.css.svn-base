body {
    background-color: #FFFFFF;
    color: #000000;
    font-family: Helvetica,Arial,sans-serif;
    font-size:xx-small;
    margin: 3px 8px;
}

html, body {
    height: auto;
    margin: 0;
    padding: 0;
}

.MIG {
    background-image: url('../images/logo_small.png');
    background-repeat: no-repeat;
    background-position: 15 0;
    background-color: transparent;
    height: 75px;
    overflow: hidden;
    position: absolute;
    width: 140px;
}

.MJG {
    margin: 0.2em;
    text-align: right;
}

/* Demos */

.demos-nav, .demos-nav dt, .demos-nav dd, .demos-nav ul, .demos-nav li {
	margin: 0;
	padding: 0
}

.demos-nav {
	float: left;
	width: 170px;
	font-size: 1em;
}

.demos-nav dt,
.demos-nav h4 {
	margin: 0;
	padding: 0;
	font: normal 1.1em "Trebuchet MS", "Helvetica", "Arial",  "Verdana", "sans-serif";
	color: #e87b10;
}

.demos-nav dt,
.demos-nav h4 {
	margin-top: 0.4em;
	margin-bottom: 0;
	padding-left: 8px;
	padding-bottom:5px;
	line-height: 1.2em;
	border-bottom: 1px solid #F4F4F4;
}

.demos-nav dd a,
.demos-nav li a {
	border-bottom: 1px solid #F4F4F4;
	display:block;
	padding: 4px 3px 4px 8px;
	font-size: 80%;
	text-decoration: none;
	color: #555 ;
	margin:2px 0;
	height:13px;
}

.demos-nav dd a:hover,
.demos-nav dd a:focus,
.demos-nav dd a:hover,
.demos-nav dd a:focus {
	background: url("../images/highlight_arrow.gif") no-repeat scroll right center #f9a736;

	color:#000;
	-moz-border-radius: 5px; -webkit-border-radius: 5px;
}
 .demos-nav dd a.selected {
    background: url("../images/highlight_arrow.gif") no-repeat scroll right center #f9a736;
	color:#ffffff;
	-moz-border-radius: 5px; -webkit-border-radius: 5px;
}

