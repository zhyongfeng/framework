<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.fengfan.model">
	<class
		name="ActiveCall"
		table="active_call"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="string"
			column="SessionId"
		>
			<generator class="assigned"/>
		</id>

		<property
			name="StartTime"
			column="StartTime"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="EndTime"
			column="EndTime"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="CustName"
			column="CustName"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="ProviderId"
			column="ProviderId"
			type="string"
			not-null="false"
			length="50"
		/>
		<many-to-one name="Provider" column="ProviderId" class="Provider" insert="false" update="false" lazy="false"/>
		<property
			name="Realm"
			column="Realm"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="ProxyState"
			column="ProxyState"
			type="string"
			not-null="false"
			length="100"
		/>
		<property
			name="CalledID"
			column="CalledID"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="CallingID"
			column="CallingID"
			type="string"
			not-null="false"
			length="24"
		/>
		<property
			name="FramedIp"
			column="FramedIp"
			type="string"
			not-null="false"
			length="15"
		/>
		<property
			name="NasId"
			column="NasId"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NasIp"
			column="NasIp"
			type="string"
			not-null="false"
			length="50"
		/>
		<property
			name="NasPort"
			column="NasPort"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="NasPortType"
			column="NasPortType"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="BillingType"
			column="BillingType"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="Status"
			column="Status"
			type="string"
			not-null="false"
			length="1"
		/>
		<property
			name="NasPortId"
			column="NasPortId"
			type="string"
			not-null="false"
			length="80"
		/>
		<property
			name="AcctStatusType"
			column="AcctStatusType"
			type="integer"
			not-null="false"
			length="11"
		/>


	</class>	
</hibernate-mapping>