<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.fengfan.model">
	<class
		name="PayRecord"
		table="pay_record"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="Id"
			type="integer"
			column="PayId"
		>
			<generator class="identity"/>
		</id>

		<property
			name="CustId"
			column="CustId"
			type="string"
			not-null="false"
			length="32"
		/>
		<property
			name="ProviderId"
			column="ProviderId"
			type="string"
			not-null="false"
			length="50"
		/>
		<many-to-one name="Provider" column="ProviderId" class="Provider" insert="false" update="false" lazy="false"/>
		<property
			name="TollId"
			column="TollId"
			type="string"
			not-null="false"
			length="15"
		/>
		<many-to-one name="Toll" column="TollId" class="Toll" insert="false" update="false" lazy="false"/>
		<property
			name="Price"
			column="Price"
			type="java.lang.Double"
			not-null="false"
			length="11"
		/>
		<property
			name="Amount"
			column="Amount"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="Discount"
			column="Discount"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="TotalAmount"
			column="TotalAmount"
			type="java.lang.Double"
			not-null="false"
			length="11"
		/>
		<property
			name="PayDate"
			column="PayDate"
			type="timestamp"
			not-null="false"
			length="19"
		/>
		<property
			name="PayType"
			column="PayType"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="InvoiceName"
			column="InvoiceName"
			type="string"
			not-null="false"
			length="255"
		/>
		<property
			name="IsInvoiced"
			column="IsInvoiced"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="IsSettled"
			column="IsSettled"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="IsPrinted"
			column="IsPrinted"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="Remark"
			column="Remark"
			type="string"
			not-null="false"
			length="60"
		/>
		<property
			name="UpdateUser"
			column="UpdateUser"
			type="string"
			not-null="false"
			length="20"
		/>
		<property
			name="UpdateTime"
			column="UpdateTime"
			type="timestamp"
			not-null="false"
		/>
		<property
			name="UpdateContent"
			column="UpdateContent"
			type="string"
			not-null="false"
			length="255"
		/><property
			name="UpdateReason"
			column="UpdateReason"
			type="string"
			not-null="false"
			length="255"
		/>


	</class>	
</hibernate-mapping>