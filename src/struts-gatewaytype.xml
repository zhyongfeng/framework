<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN" "http://struts.apache.org/dtds/struts-2.0.dtd" >
<struts>
    <package name="gatewaytype" extends="struts-default, json-default" >
		<action name="gatewayType_*" class="com.fengfan.action.gatewaytype.GatewayTypeAction" method="{1}">
			<result name="list">/admin/gatewaytype/gatewayType_list.jsp</result>
			<result name="edit">/admin/gatewaytype/gatewayType_edit.jsp</result>
			<result name="input">/admin/gatewaytype/gatewayType_add.jsp</result>
			<result name="add">/admin/gatewaytype/gatewayType_add.jsp</result>
			<result name="view">/admin/gatewaytype/gatewayType_view.jsp</result>
			<result name="redirect" type="redirect">gatewayType_list.action</result>
        </action>
    </package>
</struts>
